    "import cv2\n",
    "from gaussian_noise import apply_gaussian_noise\n",
    "\n",
    "def anonymize_image(path_in, path_out, sigma=15):\n",
    "    image = cv2.imread(path_in, cv2.IMREAD_GRAYSCALE)\n",
    "    noisy = apply_gaussian_noise(image, sigma=sigma)\n",
    "    cv2.imwrite(path_out, noisy)\n"
   ]
